{
  "custom/whispy": {
    "exec": "/path/to/whispy/venv/bin/python /path/to/whispy/voice_recorder.py --status",
    "exec-if": "test -f ~/.whispy_waybar",
    "return-type": "json",
    "interval": 1,
    "format": "{}",
    "on-click": "/path/to/whispy/venv/bin/python /path/to/whispy/voice_recorder.py",
    "tooltip": true
  }
}